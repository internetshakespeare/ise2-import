                  <attDef ident="exclude" mode="delete"/>
                  <attDef ident="select" mode="delete"/>
               </attList>
            </classSpec>
            
            <elementSpec ident="seg" module="linking" mode="change">
               <attList>
                  <attDef ident="part" mode="delete"/>
                  
               </attList>
            </elementSpec>

            <!--MODULE: MSDESCRIPTION-->
            <!-- used by facsimile sourceDesc -->
            <moduleRef key="msdescription"
                       include="institution locus msContents msDesc msIdentifier msItem msName objectDesc physDesc repository"/>

            <!--MODULE: NAMESDATES-->
            <!-- facsimiles using settlement, country -->
            <moduleRef key="namesdates"
                       include="addName country forename listOrg listPerson persName person settlement surname org orgName"/>
            <classSpec ident="att.datable.iso"
                       type="atts"
                       module="namesdates"
                       mode="delete"/>
            
            <!--We currently do not need custom date attributes-->
            <classSpec ident="att.datable.custom"
                       type="atts"
                       module="namesdates"
                       mode="delete"/>

            <moduleRef key="tagdocs" include="att code egXML gi val ident"/>
            <elementSpec ident="egXML" module="tagdocs" mode="change">
               <attList>
                  <attDef ident="rend" mode="delete"/>
                  <attDef ident="style" mode="delete"/>
                  <attDef ident="rendition" mode="delete"/>
                  <attDef ident="corresp" mode="delete"/>
                  <attDef ident="resp" mode="delete"/>
                  <attDef ident="cert" mode="delete"/>
                  <attDef ident="source" mode="delete"/>
                  <attDef ident="facs" mode="delete"/>
                  <!--Need to know validity of the EGXML-->
                  <attDef ident="valid" mode="change" usage="req"/>
               </attList>
            </elementSpec>

            <!--MODULE: TEI-->
            <moduleRef key="tei"/>
            <classSpec type="atts" module="tei" ident="att.canonical" mode="change">
               <attList>
                  <attDef ident="key" mode="delete"/>
               </attList>
            </classSpec>
            <classSpec ident="att.ranging" module="tei" type="atts" mode="delete"/>
            <classSpec ident="att.written" type="atts" module="tei" mode="delete"/>
            <classSpec ident="att.cReferencing" type="atts" module="tei" mode="delete"/>
            <classSpec ident="att.datable" module="tei" type="atts" mode="change">
               <attList>
                  <attDef ident="period" mode="delete"/>
               </attList>
            </classSpec>
            <classSpec ident="att.datcat" module="tei" type="atts" mode="delete"/>
            <classSpec ident="att.declarable" module="tei" type="atts" mode="delete"/>
            <classSpec ident="att.declaring" module="tei" type="atts" mode="delete"/>
            <classSpec ident="att.divLike" module="tei" type="atts" mode="delete"/>
            <classSpec ident="att.docStatus" module="tei" type="atts" mode="change">
               <attList>
                  <attDef ident="status" mode="change">
                     <valList>
                        <valItem ident="converted">
                           <gloss>This document was converted to TEI from an older format by a conversion script and has not yet been touched by a human.</gloss>
                        </valItem>
                        <valItem ident="draft">
                           <gloss>This document is currently being edited. It is not ready for publication or peer-review.</gloss>
                        </valItem>
                        <valItem ident="published">
                           <gloss>This document is ready to be published.</gloss>
                        </valItem>
                        <valItem ident="deprecated">
                           <gloss>This document is no longer relevant, but is being preserved for archival purposes.</gloss>
                        </valItem>
                     </valList>
                  </attDef>
               </attList>
            </classSpec>
            <classSpec ident="att.duration.w3c" module="tei" type="atts" mode="delete"/>
            <classSpec ident="att.duration.iso" module="tei" type="atts" mode="delete"/>
            <classSpec ident="att.editLike" module="tei" type="atts" mode="change">
               <attList>
                  <attDef ident="instant" mode="delete"/>
               </attList>
            </classSpec>
            <classSpec ident="att.global" module="tei" type="atts" mode="change">
               <attList>
                  <attDef ident="xml:base" mode="delete"/>
                  <attDef ident="xml:space" mode="delete"/>
               </attList>
            </classSpec>
            <classSpec ident="att.handFeatures" module="tei" type="atts" mode="delete"/>
            <classSpec ident="att.interpLike" module="tei" type="atts" mode="delete"/>
            <classSpec ident="att.measurement" module="tei" type="atts" mode="delete"/>
            <classSpec ident="att.naming" module="tei" type="atts" mode="change">
               <attList>
                  <attDef ident="nymRef" mode="delete"/>
               </attList>
            </classSpec>
            <classSpec ident="att.notation" module="tei" type="atts" mode="delete"/>
            <classSpec ident="att.pointing" module="tei" type="atts" mode="change">
               <attList>
                  <attDef ident="targetLang" mode="delete"/>
                  <attDef ident="evaluate" mode="delete"/>
               </attList>
            </classSpec>
            <classSpec ident="att.pointingGroup"
                       module="tei"
                       type="atts"
                       mode="delete"/>
            <classSpec ident="att.scoping" type="atts" module="tei" mode="delete"/>
            <classSpec ident="att.segLike" type="atts" module="tei" mode="delete"/>
            <classSpec ident="att.sortable" module="tei" type="atts" mode="delete"/>
           <!-- <classSpec ident="att.edition" module="tei" type="atts" mode="delete"/>-->
            <classSpec ident="att.readFrom" type="atts" module="tei" mode="delete"/>
            <classSpec ident="att.spanning" type="atts" module="tei" mode="delete"/>
            <classSpec ident="att.repeatable" type="atts" module="tei" mode="delete"/>
            <classSpec ident="att.timed" type="atts" module="tei" mode="delete"/>
            <classSpec ident="att.transcriptional"
                       type="atts"
                       module="tei"
                       mode="delete"/>
            <classSpec ident="att.translatable" type="atts" module="tei" mode="delete"/>
            <classSpec ident="att.citing" type="atts" module="tei" mode="delete"/>
            <classSpec ident="att.partials" type="atts" module="tei" mode="delete"/>
            <classSpec ident="att.personal" type="atts" module="tei" mode="change">
               <attList>
                  <attDef ident="full" mode="delete"/>
                  <attDef ident="sort" mode="delete"/>
               </attList>
            </classSpec>


            <classSpec ident="att.global.analytic"
                       type="atts"
                       module="tei"
                       mode="delete"/>
            <classSpec ident="att.pointing.group"
                       type="atts"
                       module="tei"
                       mode="delete"/>






            <!--MODULE: TEXTCRIT-->
            <moduleRef key="textcrit" include="app lem listApp listWit rdg wit witness"/>
            <classSpec ident="att.textCritical"
                       module="textcrit"
                       type="atts"
                       mode="change">
               <attList>
                  <attDef ident="cause" mode="delete"/>
                  <attDef ident="varSeq" mode="delete"/>
                  <attDef ident="require" mode="delete"/>
               </attList>
            </classSpec>
            
            <!--Make @wit mandatory in rdg-->
            
            <elementSpec ident="rdg" module="textcrit" mode="change">
               <attList>
                  <attDef ident="rend" mode="delete"/>
                  <attDef ident="style" mode="delete"/>
                  <attDef ident="rendition" mode="delete"/>
                  <attDef ident="facs" mode="delete"/>
                  <attDef ident="change" mode="delete"/>
                  <attDef ident="hand" mode="delete"/>
                  <attDef ident="wit" usage="req" mode="change"/>
                  <attDef ident="type" usage="opt" mode="change">
                     <valList>
                        <valItem ident="substantive">
                           <gloss>A substantive reading.</gloss>
                        </valItem>
                     </valList>
                  </attDef>
               </attList>
            </elementSpec>
            
            <!--Heavily constrain lem-->            
            <elementSpec ident="lem" module="textcrit" mode="change">
               <content>
                  <alternate minOccurs="0" maxOccurs="unbounded">
                     <textNode/>
                     <elementRef key="gap" minOccurs="0" maxOccurs="1"/>
                     <elementRef key="note" minOccurs="0" maxOccurs="unbounded"/>
                     <elementRef key="lb" minOccurs="0" maxOccurs="unbounded"/>
                  </alternate>
               </content>
            </elementSpec>
            
            <!--Make @from mandatory on apps and spans-->
            
            <elementSpec ident="app" module="textcrit" mode="change">
               <attList>
                  <attDef ident="corresp" mode="delete"/>
                  <attDef ident="facs" mode="delete"/>
                  <attDef ident="next" mode="delete"/>
                  <attDef ident="prev" mode="delete"/>
                  <attDef ident="rend" mode="delete"/>
                  <attDef ident="rendition" mode="delete"/>
                  <attDef ident="style" mode="delete"/>
                  <attDef ident="loc" mode="delete"/>
                  <attDef ident="from" mode="change" usage="req"/>
                  <attDef ident="type" mode="change">
                    <valList mode="change">
                       <valItem ident="relineation">
                          <gloss>relineation</gloss>
                          <desc>a collation that records relineation</desc>
                       </valItem>
                    </valList>
                  </attDef>
               </attList>
            </elementSpec>
            
            <elementSpec ident="span" module="analysis" mode="change">
               <attList>
                  <attDef ident="from" mode="change" usage="req"/>
               </attList>
            </elementSpec>



            <!--MODULE: TEXTSTRUCTURE-->
            <moduleRef key="textstructure"
                       include="TEI back body byline closer dateline div docAuthor docDate docEdition docImprint docTitle front group opener text titlePage titlePart trailer"/>
            
            <!--MODULE: TRANSCR-->
            <!--sourceDoc and surface are used by facsimiles-->
            <moduleRef key="transcr" include="fw space supplied sourceDoc surface"/>
            <classSpec ident="att.global.change"
                       type="atts"
                       module="transcr"
                       mode="delete"/>
            <classSpec ident="att.coordinated"
                       type="atts"
                       module="transcr"
                       mode="delete"/>
          
          
            <!--Global attributes-->
            
            <classSpec module="tei"
                       ident="att.global.responsibility"
                       mode="change"
                       type="atts">
               <attList>
                  <attDef ident="cert" mode="change">
                     <datatype>
                        <dataRef key="teidata.certainty"/>
                     </datatype>
                  </attDef>
               </attList>
            </classSpec>





         </schemaSpec>
      </body>
   </text>
</TEI>
