				<!-- Document types -->
				<taxonomy xml:id="iseDocumentTypes">
					<gloss>Taxonomy of document types</gloss>
					<category xml:id="idtBornDigital" n="Born Digital">
						<catDesc>Not based on a primary source.</catDesc>
					</category>
					<category xml:id="idtExcerpted">
						<catDesc>Includes incomplete selections or segments of a primary source.</catDesc>
					</category>
					<category xml:id="idtAnthology">
						<catDesc>A collection of related primary sources.</catDesc>
					</category>
					<category xml:id="idtPrimary">
						<catDesc>A digital edition of a primary source document.</catDesc>
						<category xml:id="idtPrimaryModern">
							<catDesc>A modernized version of a primary source.</catDesc>
						</category>
						<category xml:id="idtPrimaryPlay">
							<catDesc>Conventionally considered a play.</catDesc>
						</category>
						<category xml:id="idtPrimaryPoem">
							<catDesc>Conventionally considered a poem.</catDesc>
						</category>
						<category xml:id="idtPrimaryProse">
							<catDesc>Conventionally considered non-dramatic prose.</catDesc>
						</category>
					</category>
					<category xml:id="idtDatabase">
						<catDesc>A centralised collection of items used across the site.</catDesc>
					</category>
					<category xml:id="idtParatext">
						<catDesc>Supplements an idtPrimary document.</catDesc>
						<category xml:id="idtParatextCritical">
							<catDesc>Critical material.</catDesc>
							<category xml:id="idtParatextCriticalIntroduction">
								<catDesc>Critical introduction.</catDesc>
							</category>
						</category>
						<category xml:id="idtParatextHistory">
							<catDesc>Historical overview.</catDesc>
							<category xml:id="idtParatextHistoryPerformance">
								<catDesc>Performance history.</catDesc>
							</category>
							<category xml:id="idtParatextHistoryCriticism">
								<catDesc>History of criticism.</catDesc>
							</category>
							<category xml:id="idtParatextHistoryTextual">
								<catDesc>Textual history.</catDesc>
							</category>
						</category>
						<category xml:id="idtParatextCharacters">
							<catDesc>List of dramatic personae or other literary characters.</catDesc>
						</category>
						<category xml:id="idtParatextContextual">
							<catDesc>Contextual supplement.</catDesc>
							<!-- Eg, Mardock's essay on war in the 16C for H5 -->
						</category>
						<category xml:id="idtParatextGlossary">
							<catDesc>Glossary.</catDesc>
						</category>
						<category xml:id="idtParatextChronology">
							<catDesc>List of dates and times.</catDesc>
						</category>
						<category xml:id="idtParatextPedagogical">
							<catDesc>Teaching notes, assignments, or other pedagogical engagements.</catDesc>
						</category>
					</category>
					<category xml:id="idtFacsimile">
						<catDesc>A material copy of a text.</catDesc>
						<category xml:id="idtFacsimileFolio">
							<catDesc>Facsimiles in folio format.</catDesc>
							<category xml:id="idtFacsimileFOLI1">
								<catDesc>Facsimiles of Shakespeare's First Folio.</catDesc>
							</category>
							<category xml:id="idtFacsimileFOLI2">
								<catDesc>Facsimiles of Shakespeare's Second Folio.</catDesc>
							</category>
							<category xml:id="idtFacsimileFOLI3">
